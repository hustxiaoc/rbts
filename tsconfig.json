{
  "files": [ "tree.ts", "node.ts" ],
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "outDir": "./dist",
    "stripInternal": true,
    "sourceMap": true,
    "declaration": true,
    "pretty": true,
    "strict": true
  },
  "typedocOptions": {
    "name": "Typescript Red-Black Tree",
    "out": "doc",
    "mode": "file",
    "readme": "none",
    "theme": "markdown",
    "excludeNotExported": true,
    "includeDeclarations": true,
    "exclude": "node_modules/*"
  }
}
