{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [ true, "never" ],
        "quotemark": [ true, "single" ],
        "member-access": [ false ],
        "variable-name": [ false ],
        "curly": [ false ],
        "arrow-parens": [ false ],
        "one-variable-per-declaration": [ false ],
        "one-line": [ true, "check-whitespace" ],
        "max-classes-per-file": [ true, 5 ],
        "interface-name": [ false ],
        "member-ordering": [ true, "instance-sandwich" ],
        "new-parens": [ false ],
        "no-consecutive-blank-lines": [ true, 2 ]
    },
    "rulesDirectory": []
}