{
  "name": "rbts",
  "version": "0.0.2",
  "description": "Typescript Red-Black Tree",
  "main": "dist/redblack.js",
  "types": "dist/redblack.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "tslint --project .",
    "pretest": "npm run build; npm run lint",
    "test": "nyc mocha",
    "posttest": "nyc report --reporter=text-lcov > coverage/coverage.lcov"
  },
  "author": "Daniel Ly",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/debug": "^4.1.4",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.6",
    "chai": "^4.2.0",
    "codecov": "^3.3.0",
    "debug": "^4.1.1",
    "mocha": "^6.1.4",
    "nyc": "^14.0.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.4"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nalply/rbts.git"
  },
  "keywords": [
    "red-black",
    "tree",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/nalply/rbts/issues"
  },
  "homepage": "https://github.com/nalply/rbts#readme",
  "mocha": {
    "require": "ts-node/register",
    "ui": "qunit",
    "spec": "test.ts",
    "timeout": "10s"
  },
  "nyc": {
    "include": [ "tree.ts", "node.ts" ],
    "exclude": [
      "typings", "test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  }
}
